<?xml version="1.0" encoding="UTF-8"?>
<mappings
    xmlns="http://lazydog.org/xml/ns/historytable"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://lazydog.org/xml/ns/historytable
                        http://lazydog.org/xml/ns/historytable/history-table-mappings.xsd">

    <entity-table-mapping>
        <entity-class>org.lazydog.comic.api.model.Comic</entity-class>
        <property-column-mapping>
            <entity-class-property>id</entity-class-property>
        </property-column-mapping>
        <property-column-mapping>
            <entity-class-property>title.id</entity-class-property>
        </property-column-mapping>
        <property-column-mapping>
            <entity-class-property>startDate</entity-class-property>
        </property-column-mapping>
    </entity-table-mapping>
    <entity-table-mapping>
        <entity-class>org.lazydog.comic.api.model.ComicType</entity-class>
        <property-column-mapping>
            <entity-class-property>id</entity-class-property>
        </property-column-mapping>
    </entity-table-mapping>
</mappings>
